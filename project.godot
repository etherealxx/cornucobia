; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cornucobia"
config/version="0.0.8"
run/main_scene="res://scenes/maintest_real.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://cornucobia_icon.png"

[autoload]

SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
GlobalVar="*res://scripts/autoloads/GlobalVar.gd"
Shaker="*res://addons/shaker/src/Shaker.gd"
CollisionCalc="*res://scripts/autoloads/collision_calc.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=2400
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/SimpleTODO/plugin.cfg", "res://addons/shaker/plugin.cfg", "res://addons/silent_wolf/plugin.cfg", "res://addons/simple_tool_button/plugin.cfg")

